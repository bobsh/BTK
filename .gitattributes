########
# Images
########
*.jpg filter=lfs diff=lfs merge=lfs -crlf
*.JPG filter=lfs diff=lfs merge=lfs -crlf
*.png filter=lfs diff=lfs merge=lfs -crlf
*.PNG filter=lfs diff=lfs merge=lfs -crlf
*.tif filter=lfs diff=lfs merge=lfs -crlf
*.TIF filter=lfs diff=lfs merge=lfs -crlf
*.tga filter=lfs diff=lfs merge=lfs -crlf
*.TGA filter=lfs diff=lfs merge=lfs -crlf
*.bmp filter=lfs diff=lfs merge=lfs -crlf
*.BMP filter=lfs diff=lfs merge=lfs -crlf
*.gif filter=lfs diff=lfs merge=lfs -crlf
*.GIF filter=lfs diff=lfs merge=lfs -crlf
# Mac icon format
*.icns filter=lfs diff=lfs merge=lfs -crlf
*.ICNS filter=lfs diff=lfs merge=lfs -crlf
# OpenEXR (used for lightmaps)
*.exr filter=lfs diff=lfs merge=lfs -crlf
*.EXR filter=lfs diff=lfs merge=lfs -crlf
####################
# Adobe source files
####################
# Photoshop
*.psd filter=lfs diff=lfs merge=lfs -crlf
*.PSD filter=lfs diff=lfs merge=lfs -crlf
# Illustrator
*.ai filter=lfs diff=lfs merge=lfs -crlf
*.AI filter=lfs diff=lfs merge=lfs -crlf
# PDF
*.pdf filter=lfs diff=lfs merge=lfs -crlf
*.PDF filter=lfs diff=lfs merge=lfs -crlf
##################
# Compressed files
##################
*.gz filter=lfs diff=lfs merge=lfs -crlf
*.zip filter=lfs diff=lfs merge=lfs -crlf
*.ZIP filter=lfs diff=lfs merge=lfs -crlf
*.xz filter=lfs diff=lfs merge=lfs -crlf
*.7z filter=lfs diff=lfs merge=lfs -crlf
##########
# Archives
##########
*.iso filter=lfs diff=lfs merge=lfs -crlf
*.ISO filter=lfs diff=lfs merge=lfs -crlf
*.tar filter=lfs diff=lfs merge=lfs -crlf
*.TAR filter=lfs diff=lfs merge=lfs -crlf
*.cpio filter=lfs diff=lfs merge=lfs -crlf
*.CPIO filter=lfs diff=lfs merge=lfs -crlf
*.rar filter=lfs diff=lfs merge=lfs -crlf
*.RAR filter=lfs diff=lfs merge=lfs -crlf
*.jar filter=lfs diff=lfs merge=lfs -crlf
###########
# Packaging
###########
*.rpm filter=lfs diff=lfs merge=lfs -crlf
*.deb filter=lfs diff=lfs merge=lfs -crlf
*.msi filter=lfs diff=lfs merge=lfs -crlf
#######
# Fonts
#######
*.otf filter=lfs diff=lfs merge=lfs -crlf
*.OTF filter=lfs diff=lfs merge=lfs -crlf
*.ttf filter=lfs diff=lfs merge=lfs -crlf
*.TTF filter=lfs diff=lfs merge=lfs -crlf
#######
# Sound
#######
*.wav filter=lfs diff=lfs merge=lfs -crlf
*.WAV filter=lfs diff=lfs merge=lfs -crlf
*.mp3 filter=lfs diff=lfs merge=lfs -crlf
*.MP3 filter=lfs diff=lfs merge=lfs -crlf
########
# Movies
########
*.avi filter=lfs diff=lfs merge=lfs -crlf
*.AVI filter=lfs diff=lfs merge=lfs -crlf
*.wmv filter=lfs diff=lfs merge=lfs -crlf
*.WMV filter=lfs diff=lfs merge=lfs -crlf
*.webm filter=lfs diff=lfs merge=lfs -crlf
*.WEBM filter=lfs diff=lfs merge=lfs -crlf
*.mov filter=lfs diff=lfs merge=lfs -crlf
*.MOV filter=lfs diff=lfs merge=lfs -crlf
*.flv filter=lfs diff=lfs merge=lfs -crlf
*.FLV filter=lfs diff=lfs merge=lfs -crlf
*.ogv filter=lfs diff=lfs merge=lfs -crlf
*.OGV filter=lfs diff=lfs merge=lfs -crlf
*.ogg filter=lfs diff=lfs merge=lfs -crlf
*.OGG filter=lfs diff=lfs merge=lfs -crlf
##################
# Graphical Source
##################
# Gimp
*.xcf filter=lfs diff=lfs merge=lfs -crlf
# CorelDraw
*.cdr filter=lfs diff=lfs merge=lfs -crlf
*.CDR filter=lfs diff=lfs merge=lfs -crlf
# Sketch
*.sketch
#########
# Library
#########
*.so filter=lfs diff=lfs merge=lfs -crlf
*.dll filter=lfs diff=lfs merge=lfs -crlf
*.DLL filter=lfs diff=lfs merge=lfs -crlf
*.aar filter=lfs diff=lfs merge=lfs -crlf
*.AAR filter=lfs diff=lfs merge=lfs -crlf
#####
# IOS
#####
# IOS localisation
*.strings filter=lfs diff=lfs merge=lfs -crlf
############
# Keys/Certs
############
# Binary Certificate/Key files
*.cer filter=lfs diff=lfs merge=lfs -crlf
*.keystore filter=lfs diff=lfs merge=lfs -crlf
#####
# Raw
#####
# Unspecific files, usually binary
*.raw filter=lfs diff=lfs merge=lfs -crlf
*.RAW filter=lfs diff=lfs merge=lfs -crlf
digtool/build/digtool.* filter=lfs diff=lfs merge=lfs -text
